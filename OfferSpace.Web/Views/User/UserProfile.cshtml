    <!-- HEADER -->
    <header class="header-menu">
      <div class="container-custom">

        <div class="header-menu-logo">
          <a href="Home/Index">
            <span class="header-menu-logo-ico">
              <img class="header-menu-logo-ico-img" src="~/Images/logo.png" alt="">
            </span>
          </a>
        </div>

        <div class="header-menu-items">
          <div class="header-menu-items-btns">
            <button class="header-menu-items-btn news">News</button>
            <button class="header-menu-items-btn notif">Notification<i class="far fa-bell"></i><span class="badge badge-dark">19</span></button>
            <button class="header-menu-items-btn log">Log out<i class="fas fa-sign-out-alt"></i></button>
          </div>
        </div>
      </div>
    </header>

    <!-- USER BAR -->
    <div class="user-bar-block" background="~/Images/img_bg_4.jpg">
      <div class="user-block shadow-lg">
        <div class="user-photo">
          <img class="user-photo-img d-inline-block align-top rounded-circle shadow" src="~/Images/person3.jpg" alt="">
        </div>
        <div class="user-name" align="center">
          <span class="user-name-text">John Doe</span>
          <a href="#">
            <span class="user-name-block-img">
              <i class="fas fa-user-edit"></i>
            </span>
          </a>
        </div>
      </div>

      <div class="user-bar shadow-lg">
        <div class="user-bar-create-request">
          <button class="create-request-btn btn btn-outline-success" type="button">Create request</button>
        </div>
      </div>
    </div>


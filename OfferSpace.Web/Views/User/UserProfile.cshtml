@{
  Layout = null;
}

<html>

<head>
  <meta charset="UTF-8">
  <meta name="robots" content="noindex">
  <link rel="shortcut icon" type="image/x-icon" href="~/Images/offer-space-icon.ico">

  <title>OfferSpace</title>

  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
  <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.2.0/css/all.css" integrity="sha384-hWVjflwFxL6sNzntih27bfxkr27PmbbK/iSvJ+a4+0owXq79v+lsFkW54bOGbiDQ" crossorigin="anonymous">

  @Styles.Render("~/ContentUserProfilePage/css")

</head>

<body>

  <div class="loader"></div>

  <div class="page">

    @* HEADER *@
    <header class="header-menu">
      <div class="container-custom">

        <div class="header-menu-logo">
          <a href="#">
            <span class="header-menu-logo-ico">
              <img class="header-menu-logo-ico-img" src="~/Images/logo.png" alt="">
            </span>
          </a>
        </div>

        <div class="header-menu-items">
          <div class="header-menu-items-btns">
            @using (Html.BeginForm("Logout", "Account", FormMethod.Post, new { id = "logoutForm" }))
            {

              <button class="header-menu-items-btn news">News&#32;<i class="fas fa-newspaper"></i></button>
              <button class="header-menu-items-btn notif">Notification&#32;<i class="far fa-bell"></i></button>
              <a href="javascript:document.getElementById('logoutForm').submit()" class="header-menu-items-btn log"> Log out<i class="fas fa-sign-out-alt"></i></a>
              <button src="javascript:document.getElementById('logoutForm').submit()" class="header-menu-items-btn log" style="visibility:hidden;">Log out&#32;</button>
            }
          </div>
        </div>
      </div>
    </header>
    

    @* USER BAR *@
    <div class="user-bar-block" background="~/Images/img_bg_4.jpg">
      <div class="user-block">
        <div class="user-photo">
          <img class="user-photo-img d-inline-block align-top rounded-circle" src="~/Images/person3.jpg" alt="">
        </div>
        <div class="user-name" align="center">
          <span class="user-name-text">John Doe</span>
          <a href="@Url.Action("Edit", "UserProfile")">
            <span class="user-name-block-img">
              <i class="fas fa-user-edit"></i>
            </span>
          </a>
        </div>
      </div>

      <div class="user-bar">
        <div class="user-bar-create-request">
          <button class="create-request-btn btn btn-outline-success" type="button">Create request</button>
        </div>
      </div>
    </div>

    @* LISTS *@
    <div class="main-frame container-custom">
      <div class="row">

        <div class="col-xl-6 col-lg-8 col-md-12">
          <div class="main-request-lists col-12">
            <div class="row">
              <div class="status-bar">

                <div class="status-items">
                  <ul class="nav nav-tabs" id="myTab" role="tablist">
                    <li class="nav-item">
                      <a class="nav-link active" id="all-tab" data-toggle="tab" href="#home" role="tab" aria-controls="home" aria-selected="true">
                        <span class="status-content">
                          <span class="status-text">All</span>
                          <span class="badge badge-dark">19</span>
                        </span>
                      </a>
                    </li>
                    <li class="nav-item">
                      <a class="nav-link" id="new-tab" data-toggle="tab" href="#profile" role="tab" aria-controls="profile" aria-selected="false">
                        <span class="status-content">
                          <span class="status-text">New</span>
                          <span class="badge badge-dark">3</span>
                        </span>
                      </a>
                    </li>
                    <li class="nav-item">
                      <a class="nav-link" id="pending-tab" data-toggle="tab" href="#contact" role="tab" aria-controls="contact" aria-selected="false">
                        <span class="status-content">
                          <span class="status-text">Pending</span>
                          <span class="badge badge-dark">2</span>
                        </span>
                      </a>
                    </li>
                    <li class="nav-item">
                      <a class="nav-link" id="done-tab" data-toggle="tab" href="#contact" role="tab" aria-controls="contact" aria-selected="false">
                        <span class="status-content">
                          <span class="status-text">Done</span>
                          <span class="badge badge-dark">14</span>
                        </span>
                      </a>
                    </li>
                    <li class="nav-item">
                      <a class="nav-link" id="canceled-tab" data-toggle="tab" href="#contact" role="tab" aria-controls="contact" aria-selected="false">
                        <span class="status-content">
                          <span class="status-text">Canceled</span>
                          <span class="badge badge-dark">0</span>
                        </span>
                      </a>
                    </li>
                    <li class="nav-item">
                      <a class="nav-link" id="canceled-tab" data-toggle="tab" href="#contact" role="tab" aria-controls="contact" aria-selected="false">
                        <span class="status-content">
                          <span class="status-text">Archived</span>
                          <span class="badge badge-dark">0</span>
                        </span>
                      </a>
                    </li>
                  </ul>
                </div>

                <div class="status-filters">
                  <div class="filter status-block dropdown">
                    <div class="status-block-img">
                      <i class="fas fa-filter"></i>
                    </div>

                    <div class="dropdown-content dropdown-menu">
